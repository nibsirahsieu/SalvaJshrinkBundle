{
	"name": "salva/jshrink-bundle",
	"type": "symfony-bundle",
	"description": "This bundle integrate jshrink library as Assetic filter and twig extension",
	"keywords": ["Assetic", "Symfony Bundle", "jshrink", "javascript", "minify", "twig"],
	"homepage": "https://github.com/nibsirahsieu/SalvaJshrinkBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Nibsirahsieu",
			"email": "nibsirahsieu@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/assetic-bundle": "2.*",
		"twig/extensions": "~1",
		"tedivm/jshrink": "0.5.*"
	},
	"autoload": {
		"psr-0": { "Salva\\JshrinkBundle": "" }
	},
	"target-dir": "Salva/JshrinkBundle",
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
